
; DRY MARTINI v2.1

[ defaults ]
; nbfunc comb-rule gen-pairs fudgeLJ fudgeQQ
  1      2         no        1.0     1.0

;       name           sig    eps

[ atomtypes ]
; name    mass chrg ptype sig eps
  Qda     72.0 0.0  A     0.0 0.0   # regular beads
  Qd      72.0 0.0  A     0.0 0.0
  Qa      72.0 0.0  A     0.0 0.0
  Q0      72.0 0.0  A     0.0 0.0
  P5      72.0 0.0  A     0.0 0.0


[ nonbond_params ]
;  part part   func level
    Qda Qda    1    0.6000 2.7000   # regular/regular, charge/charge interactions
    Qda Qd     1    0.6000 2.7000   # regular/regular, charge/charge interactions
    Qda Qa     1    0.6000 2.7000   # regular/regular, charge/charge interactions
    Qda Q0     1    0.6000 2.0000
    Qda P5     1    0.4700 0.5000

